 endpoint. Learn more: https://bit.ly/CRA-vitals\n",[],"/Users/matsudarisa/dev/calendar2/frontend/src/App.tsx",["56","57"],[],"import React, { useState, useEffect, createContext } from \"react\"\nimport { BrowserRouter as Router, Routes, Route, useNavigate } from \"react-router-dom\"\n\nimport CommonLayout from \"components/layouts/CommonLayout\"\nimport Home from \"components/pages/Home\"\nimport SignUp from \"components/pages/SignUp\"\nimport SignIn from \"components/pages/SignIn\"\n\nimport { getCurrentUser } from \"lib/api/auth\"\nimport { User } from \"interfaces/index\"\n\n// グローバルで扱う変数・関数\nexport const AuthContext = createContext({} as {\n  loading: boolean\n  setLoading: React.Dispatch<React.SetStateAction<boolean>>\n  isSignedIn: boolean\n  setIsSignedIn: React.Dispatch<React.SetStateAction<boolean>>\n  currentUser: User | undefined\n  setCurrentUser: React.Dispatch<React.SetStateAction<User | undefined>>\n})\n\nconst App: React.FC = () => {\n  const [loading, setLoading] = useState<boolean>(true)\n  const [isSignedIn, setIsSignedIn] = useState<boolean>(false)\n  const [currentUser, setCurrentUser] = useState<User | undefined>()\n\n  // const navigate = useNavigate()\n\n  // 認証済みのユーザーがいるかどうかチェック\n  // 確認できた場合はそのユーザーの情報を取得\n  const handleGetCurrentUser = async () => {\n    try {\n      const res = await getCurrentUser()\n\n      if (res?.data.isLogin === true) {\n        setIsSignedIn(true)\n        setCurrentUser(res?.data.data)\n\n        console.log(res?.data.data)\n      } else {\n        console.log(\"No current user\")\n      }\n    } catch (err) {\n      console.log(err)\n    }\n\n    setLoading(false)\n  }\n\n  useEffect(() => {\n    handleGetCurrentUser()\n  }, [setCurrentUser])\n\n\n  // ユーザーが認証済みかどうかでルーティングを決定\n  // 未認証だった場合は「/signin」ページに促す\n  const Private = ({ children }: { children: React.ReactElement }) => {\n    if (!loading) {\n      if (isSignedIn) {\n        return children\n      } else {\n        return console.log('signin')\n      }\n    } else {\n      return <></>\n    }\n  }\n\n  return (\n    <Router>\n      <AuthContext.Provider value={{ loading, setLoading, isSignedIn, setIsSignedIn, currentUser, setCurrentUser}}>\n        <CommonLayout>\n          <Routes>\n            <Route path=\"/signup\" element={<SignUp />} />\n            <Route path=\"/signin\" element={<SignIn />} />\n            <Route path=\"/\" element={<Home />} />\n          </Routes>\n        </CommonLayout>\n      </AuthContext.Provider>\n    </Router>\n  )\n}\n\nexport default App","/Users/matsudarisa/dev/calendar2/frontend/src/lib/api/auth.ts",[],[],"/Users/matsudarisa/dev/calendar2/frontend/src/components/layouts/CommonLayout.tsx",[],[],"/Users/matsudarisa/dev/calendar2/frontend/src/components/pages/SignUp.tsx",[],[],"/Users/matsudarisa/dev/calendar2/frontend/src/components/pages/Home.tsx",[],[],"/Users/matsudarisa/dev/calendar2/frontend/src/components/pages/SignIn.tsx",[],[],"/Users/matsudarisa/dev/calendar2/frontend/src/lib/api/client.ts",[],[],"/Users/matsudarisa/dev/calendar2/frontend/src/components/layouts/Header.tsx",[],[],"/Users/matsudarisa/dev/calendar2/frontend/src/components/utils/AlertMessage.tsx",[],[],{"ruleId":"58","severity":1,"message":"59","line":2,"column":8,"nodeType":"60","messageId":"61","endLine":2,"endColumn":16},{"ruleId":"58","severity":1,"message":"62","line":2,"column":50,"nodeType":"60","messageId":"61","endLine":2,"endColumn":61},{"ruleId":"58","severity":1,"message":"63","line":57,"column":9,"nodeType":"60","messageId":"61","endLine":57,"endColumn":16},"@typescript-eslint/no-unused-vars","'ReactDOM' is defined but never used.","Identifier","unusedVar","'useNavigate' is defined but never used.","'Private' is assigned a value but never used."]